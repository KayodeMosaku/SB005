SELECT * FROM EMPLOYEE_DETAILS;
SELECT * FROM EMPLOYEE_COURSE;
SELECT * FROM EMPLOYEE_EMAILS;
SELECT * FROM EMPLOYEE_MARKS;
SELECT * FROM EMPLOYEE_PHONE;
SELECT * FROM EMPLOYEE_REFERANCE;



Hibernate: insert into EMPLOYEE_DETAILS (EMPLOYEE_NAME, EMPLOYEE_DOB, EMPLOYEE_QUALIFICATION, EMPLOYEE_ID) values (?, ?, ?, ?)
Hibernate: insert into EMPLOYEE_COURSE (empid, idx, COURSE) values (?, ?, ?)
Hibernate: insert into EMPLOYEE_EMAILS (empid, idx, EMAILS) values (?, ?, ?)
Hibernate: insert into EMPLOYEE_MARKS (empid, MARKS) values (?, ?)
Hibernate: insert into EMPLOYEE_PHONE (empid, PHONES) values (?, ?)
Hibernate: insert into EMPLOYEE_REFERANCE (empid, REF_NAME, REF_PHONE) values (?, ?, ?)
